<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
	<managed-bean>
		<managed-bean-name>loginBean</managed-bean-name>
		<managed-bean-class>org.govmix.proxy.igrue.web.mbean.LoginBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>loginDao</property-name>
			<property-class>org.govmix.proxy.igrue.web.service.ILoginDAO</property-class>
			<value>#{loginDAO}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>trasmissioniBean</managed-bean-name>
		<managed-bean-class>org.govmix.proxy.igrue.web.mbean.TrasmissioniBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>service</property-name>
			<property-class>org.govmix.proxy.igrue.web.service.IService</property-class>
			<value>#{trasmissioniService}</value>
		</managed-property>
		<managed-property>
			<property-name>eventiService</property-name>
			<property-class>org.govmix.proxy.igrue.web.service.IEvento</property-class>
			<value>#{eventiService}</value>
		</managed-property>
		<managed-property>
			<property-name>esitiService</property-name>
			<property-class>org.govmix.proxy.igrue.web.service.IEsito</property-class>
			<value>#{esitiService}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>trasmissioniDataModel</managed-bean-name>
		<managed-bean-class>org.govmix.proxy.igrue.web.datamodel.TrasmissioniDataModel</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>dataProvider</property-name>
			<property-class>org.govmix.proxy.igrue.web.service.ITrasmissione</property-class>
			<value>#{trasmissioniService}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>eventiDataModel</managed-bean-name>
		<managed-bean-class>org.govmix.proxy.igrue.web.datamodel.EventiDataModel</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>dataProvider</property-name>
			<property-class>org.govmix.proxy.igrue.web.service.IEvento</property-class>
			<value>#{eventiService}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>tabelleContestoBean</managed-bean-name>
		<managed-bean-class>org.govmix.proxy.igrue.web.mbean.TabellaContestoBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>service</property-name>
			<property-class>org.govmix.proxy.igrue.web.service.IService</property-class>
			<value>#{tabellaContestoService}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>eventiBean</managed-bean-name>
		<managed-bean-class>org.govmix.proxy.igrue.web.mbean.EventiBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>service</property-name>
			<property-class>org.govmix.proxy.igrue.web.service.IService</property-class>
			<value>#{eventiService}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>tabellaContestoDataModel</managed-bean-name>
		<managed-bean-class>org.govmix.proxy.igrue.web.datamodel.TabellaContestoDataModel</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>dataProvider</property-name>
			<property-class>org.govmix.proxy.igrue.web.service.ITabellacontesto</property-class>
			<value>#{tabellaContestoService}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>avvisiEmailDataModel</managed-bean-name>
		<managed-bean-class>org.govmix.proxy.igrue.web.datamodel.AvvisiEmailDataModel</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>dataProvider</property-name>
			<property-class>org.govmix.proxy.igrue.web.service.IMailTemplate</property-class>
			<value>#{avvisiEmailService}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>avvisiEmailBean</managed-bean-name>
		<managed-bean-class>org.govmix.proxy.igrue.web.mbean.AvvisiEmailBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>service</property-name>
			<property-class>org.govmix.proxy.igrue.web.service.IService</property-class>
			<value>#{avvisiEmailService}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>codiceAttivazioneBean</managed-bean-name>
		<managed-bean-class>org.govmix.proxy.igrue.web.mbean.CodiceAttivazioneBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>service</property-name>
			<property-class>org.govmix.proxy.igrue.web.service.IRichiestaCodiceAttivazione</property-class>
			<value>#{codiceAttivazioneService}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>avvisiInviatiDataModel</managed-bean-name>
		<managed-bean-class>org.govmix.proxy.igrue.web.datamodel.AvvisiInviatiDataModel</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>dataProvider</property-name>
			<property-class>org.govmix.proxy.igrue.web.service.IMailTrace</property-class>
			<value>#{avvisiInviatiService}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>avvisiInviatiBean</managed-bean-name>
		<managed-bean-class>org.govmix.proxy.igrue.web.mbean.AvvisiInviatiBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>service</property-name>
			<property-class>org.govmix.proxy.igrue.web.service.IService</property-class>
			<value>#{avvisiInviatiService}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>utentiBean</managed-bean-name>
		<managed-bean-class>org.govmix.proxy.igrue.web.mbean.UtentiBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>service</property-name>
			<property-class>org.govmix.proxy.igrue.web.service.IUserService</property-class>
			<value>#{userService}</value>
		</managed-property>
	</managed-bean>
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>nuovaUtenza</from-outcome>
			<to-view-id>/pages/form/utente.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>welcome</from-outcome>
			<to-view-id>/pages/listaTrasmissioni.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/pages/login.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>listaTrasmissioni</from-outcome>
			<to-view-id>/pages/listaTrasmissioni.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>tabellaContestoList</from-outcome>
			<to-view-id>/pages/tabellaContestoList.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>richiestaCodice</from-outcome>
			<to-view-id>/pages/richiestaCodice.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>configAvvisi</from-outcome>
			<to-view-id>/pages/configAvvisi.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>eventiList</from-outcome>
			<to-view-id>/pages/eventiList.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>trasmissioneAdd</from-outcome>
			<to-view-id>/pages/trasmissioneAdd.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>listaAvvisi</from-outcome>
			<to-view-id>/pages/listaAvvisi.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>loginSuccess</from-outcome>
			<to-view-id>/pages/listaTrasmissioni.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/listaTrasmissioni.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>dettagliTrasmissione</from-outcome>
			<to-view-id>/pages/dettagliTrasmissione.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/form/utente.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>creazioneUtenzaOk</from-outcome>
			<to-view-id>/pages/list/utenti.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<application>
		<view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
		<locale-config>
			<default-locale>it</default-locale>
			<supported-locale>it</supported-locale>
		</locale-config>
		<resource-bundle>
			<base-name>appInfo</base-name>
			<var>appinfo</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>app_prop</base-name>
			<var>app_prop</var>
		</resource-bundle>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
	</application>
	<lifecycle>
		<phase-listener id="loginPhaseListener">org.govmix.proxy.igrue.web.listener.LoginPhaseListener</phase-listener>
	</lifecycle>
</faces-config>
